		"@vitest/ui": "^3.2.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-svelte": "^2.45.1",
		"flowbite-svelte-icons": "^2.0.2",
		"jsdom": "^25.0.1",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"query-string": "^9.2.2",
