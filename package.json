{
	"name": "liqueur-solutions",
	"version": "0.1.0",
	"private": "true",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chadkirby/liqueur-solutions.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"tsnode": "node --import tsx/esm"
	},
	"bugs": {
		"url": "https://github.com/chadkirby/liqueur-solutions/issues"
	},
	"exports": {
		".": {
			"import": "./esm/src/solutions.js",
			"require": "./script/solutions.js"
		}
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tsconfig/svelte": "^5.0.4",
		"@types/lodash.debounce": "^4.0.7",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.45.1",
		"fflate": "^0.8.2",
		"flowbite-svelte-icons": "^2.0.2",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"query-string": "^9.0.0",
		"svelte": "^5.0.0",
		"svelte-5-ui-lib": "^0.10.3",
		"svelte-check": "^4.0.0",
		"svelte-number-spinner": "file:../svelte-number-spinner",
		"svelte-preprocess": "^6.0.0",
		"svelte-rune-highlight": "^0.5.13",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"tsx": "^4.7.2",
		"typescript": "^5.5.0",
		"vite": "^5.4.4",
		"vitest": "^1.0.0"
	},
	"volta": {
		"node": "20.12.1"
	},
	"dependencies": {
		"lodash.debounce": "^4.0.8"
